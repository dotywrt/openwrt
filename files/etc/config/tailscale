
config tailscale 'settings'
	option enabled '1'
	option port '41641'
	option config_path '/etc/tailscale'
	option fw_mode 'nftables'
	option log_stdout '1'
	option log_stderr '1'
	option acceptRoutes '1'
	option acceptDNS '1'
	option advertiseExitNode '1'
	list advertiseRoutes '192.168.1.0/24'
	option s2s '1'
	option accept_routes '0'
	option accept_dns '1'
	option advertise_exit_node '0'

